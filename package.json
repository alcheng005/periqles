{
  "name": "periqles",
  "version": "0.1.0",
  "description": "React component library to generate forms from a GraphQL schema",
  "main": "/lib/js/periqles-min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/periqles"
  },
  "keywords": [
    "react",
    "relay",
    "apollo",
    "graphql",
    "forms",
    "component"
  ],
  "scripts": {
